{
    "name": "mohamed7sameer/backpack6-shop",
    "description": "Webshop crud models and functionality for Backpack for Laravel. Love it.",
    "license": "MIT",
    "authors": [
        {
            "name": "mohammed-samir",
            "email": "info@mohammed-samir.com",
            "homepage": "https://mohammed-samir.com"
        }
    ],
    "homepage": "https://github.com/mohamed7sameer/backpack6-shop",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "BackpackShop"
    ],
    "require": {
        "backpack/crud": "*",
        "backpack/pro": "*",
        "barryvdh/laravel-dompdf": "^2.0",
        "illuminate/support": "*",
        "mohamed7sameer/backpack-image-traits": "*",
        "cviebrock/eloquent-sluggable" : "*",
        "symfony/intl": "*",

        "intervention/image": "*"

    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4": {
            "mohamed7sameer\\BackpackShop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mohamed7sameer\\BackpackShop\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "mohamed7sameer\\BackpackShop\\AddonServiceProvider"
            ]
        }
    }
}
